
visu:CentralTendencyMeasureTaskMethodShape a sh:NodeShape ;
    sh:targetClass stats:CentralTendencyMeasure ;
    sh:property [
        sh:path visu:hasCentralTendencyMeasureMethod ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:or (
            [ sh:class stats:MeanMethod ]
        ) ;
        sh:message "CentralTendencyMeasure must be connected with one compatible atomic method." ;
    ] .

